<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.short_chains.repository.ChainInfoMapper">

    <select id="querySortChainUrlList" resultType="String">
        select sort_chain_url
        from chain_info
        where status = '1';
    </select>

</mapper>